  "description": "Date converter",
  "author": "Semen Loktionov <loktionov.semen.kh@gmail.com>",
  "private": false,
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "scripts": {
    "serve": "vue-cli-service serve",
